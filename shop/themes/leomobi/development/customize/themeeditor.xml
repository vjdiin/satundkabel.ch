<?xml version="1.0" encoding="UTF-8" ?>
<themeeditor>
	<version value="1.4">
	<ccc available="true" />
	<guest_checkout available="true" />
	<one_page_checkout available="true" />
	<store_locator available="true" />
	</version>
	<author>leotheme</author>
<name>Live Theme Editor</name>	
<selectors>
	<items>
		   <header>Body Content</header>
		   <match>body</match>

		  <item>
			<type>image</type>
			<label>Background Image</label>
			<selector>body</selector>
			<attrs>background-image</attrs>
		 </item> 

		 <item>
			<type>text</type>
			<label>Background Content</label>
			<selector>body #page</selector>
			<attrs>background-color</attrs>
		</item> 
			<item>
				<type>image</type>
				<label>Background Image</label>
				<selector>body #page</selector>
				<attrs>background-image</attrs>
			</item> 
		<item>
			<type>text</type>
				<label>Container Background</label>
				<selector>.container</selector>
				<attrs>background-color</attrs>
		</item> 
	 	<item>
			<type>fontsize</type>
			<label>Font-Size</label>
			<selector>body #page</selector>
			<attrs>font-size</attrs>
		</item> 


		<item>
			<type>text</type>
			<label>Color Text</label>
			<selector>body #page</selector>
			<attrs>color</attrs>
		</item> 
		<item>
			<type>text</type>
			<label>Link Color</label>
			<selector>body #page a</selector>
			<attrs>color</attrs>
		</item> 

		<item>
			<type>text</type>
			<label>Link Hover Color</label>
			<selector>a:hover</selector>
			<attrs>color</attrs>
		</item>
	</items>
	<items>
		<header>TopBar</header>
		<match>topbar</match>
		<item>
			<type>text</type>
			<label>Background Color</label>
			<selector>#topbar .container</selector>
			<attrs>background-color</attrs>
		</item> 
		<item>
			<type>text</type>
			<label>Color Border</label>
			<selector>#topbar .row</selector>
			<attrs>border-color</attrs>
		</item> 
	 	<item>
			<type>text</type>
			<label>Color Text</label>
			<selector>#topbar</selector>
			<attrs>color</attrs>
		</item> 

	 	<item>
			<type>text</type>
			<label>Color Link</label>
			<selector>#topbar a</selector>
			<attrs>color</attrs>
		</item> 
		<item>
			<type>text</type>
			<label>Color Link Hover</label>
			<selector>#topbar a:hover, header .nav a:hover, header .nav a.active</selector>
			<attrs>color</attrs>
		</item> 

	</items>
	<items>
		<header>Header</header>
		<match>header-main</match>
		<item>
			<type>text</type>
			<label>Background Color</label>
			<selector>#header</selector>
			<attrs>background-color</attrs>
		</item> 
	 	<item>
			<type>text</type>
			<label>Color Text</label>
			<selector>#header</selector>
			<attrs>color</attrs>
		</item> 

	 	<item>
			<type>text</type>
			<label>Color Link</label>
			<selector>#header a</selector>
			<attrs>color</attrs>
		</item> 
		<item>
			<type>text</type>
			<label>Color Link Hover</label>
			<selector>#header a:hover</selector>
			<attrs>color</attrs>
		</item> 
		<item>
			<type>text</type>
			<label>Background Input</label>
			<selector>#search_block_top #search_query_top</selector>
			<attrs>background-color</attrs>
		</item> 
		<item>
			<type>text</type>
			<label>Color Text Input</label>
			<selector>#search_block_top input.form-control </selector>
			<attrs>color</attrs>
		</item>

	</items>
	<items>
		<header>MainMenu</header>
		<match>mainnav</match>
		<item>
			<type>text</type>
			<label>Background Color</label>
			<selector>#cavas_menu</selector>
			<attrs>background-color</attrs>
		</item> 
		<item>
			<type>text</type>
			<label>Border Color</label>
			<selector>#leo-top-menu .nav > li</selector>
			<attrs>border-color</attrs>
		</item> 
	 	<item>
			<type>text</type>
			<label>Color Link</label>
			<selector>#cavas_menu a</selector>
			<attrs>color</attrs>
		</item> 
		<item>
			<type>text</type>
			<label>Color Link Hover</label>
			<selector>#leo-top-menu .dropdown-menu a:hover</selector>
			<attrs>color</attrs>
		</item> 

	</items>

	<items>
		<header>Footer Top</header>
		<match>footer-top</match>
			<item>
				<type>text</type>
				<label>Background Color</label>
				<selector>#footertop .container</selector>
				<attrs>background-color</attrs>
			</item> 
			<item>
				<type>text</type>
				<label>Color Text</label>
				<selector>#footertop</selector>
				<attrs>color</attrs>
			</item> 

			<item>
				<type>text</type>
				<label>Color Link</label>
				<selector>#footertop a</selector>
				<attrs>color</attrs>
			</item> 
			<item>
				<type>text</type>
				<label>Color Link Hover</label>
				<selector>#footertop a:hover</selector>
				<attrs>color</attrs>
			</item> 

			<item>
				<type>image</type>
				<label>Background Image</label>
				<selector>#footertop</selector>
				<attrs>background-image</attrs>
			</item>
	</items>
	<items>
		<header>Footer Center</header>
		<match>footer-center</match>
			<item>
				<type>text</type>
				<label>Background Color</label>
				<selector>#footercenter .container</selector>
				<attrs>background-color</attrs>
			</item> 
			<item>
				<type>text</type>
				<label>Color Text</label>
				<selector>#footercenter</selector>
				<attrs>color</attrs>
			</item> 

			<item>
				<type>text</type>
				<label>Color Link</label>
				<selector>#footercenter a</selector>
				<attrs>color</attrs>
			</item> 
			<item>
				<type>text</type>
				<label>Color Link Hover</label>
				<selector>#footercenter a:hover</selector>
				<attrs>color</attrs>
			</item> 
			<item>
				<type>text</type>
				<label>Color Border</label>
				<selector>.footer-container #footercenter .container > div</selector>
				<attrs>border-color</attrs>
			</item> 
			<item>
				<type>image</type>
				<label>Background Image</label>
				<selector>#footercenter .container</selector>
				<attrs>background-image</attrs>
			</item>
	</items>
	<items>
		<header>Powered</header>
		<match>powered</match>
		<item>
			<type>text</type>
			<label>Background Color</label>
			<selector>#footernav .container > div.inner</selector>
			<attrs>background-color</attrs>
		</item> 
		<item>
			<type>text</type>
			<label>Color Text</label>
			<selector>#footernav .container</selector>
			<attrs>color</attrs>
		</item> 

	 	<item>
			<type>text</type>
			<label>Color Link</label>
			<selector>#footernav .container a</selector>
			<attrs>color</attrs>
		</item> 
		<item>
			<type>text</type>
			<label>Color Link Hover</label>
			<selector>#footernav .container a:hover</selector>
			<attrs>color</attrs>
		</item> 
		<item>
			<type>image</type>
			<label>Background Image</label>
			<selector>#footernav .container > div.inner</selector>
			<attrs>background-image</attrs>
		</item> 

	</items>
</selectors>

<elements>
	<items>
		<item>
			<type>text</type>
			<label>Color Name Product</label>
			<selector>.pb-center-column h1</selector>
			<attrs>color</attrs>
		</item> 
		<header>Products</header>
		<match>product</match>
		<item>
			<type>text</type>
			<label>Color Price</label>
			<selector>.box-info-product .price</selector>
			<attrs>color</attrs>
		</item> 
 		<item>
			<type>text</type>
			<label>Color Price Old</label>
			<selector>.box-info-product #old_price_display</selector>
			<attrs>color</attrs>
		</item> 
		<item>
			<type>text</type>
			<label>BgColor Cart</label>
			<selector>.box-info-product .exclusive</selector>
			<attrs>background-color</attrs>
		</item>
		<item>
			<type>text</type>
			<label>Color Cart</label>
			<selector>.box-info-product .exclusive</selector>
			<attrs>color</attrs>
		</item> 
		<item>
			<type>text</type>
			<label>Color Wishlist</label>
			<selector>.box-info-product #wishlist_button, .box-info-product #wishlist_button:before</selector>
			<attrs>color</attrs>
		</item>

	</items>
	<items>
		<header>Modules default in Sidebar</header>
		<match>modules</match>
			<item>
				<type>text</type>
				<label>Heading Background</label>
				<selector>.sidebar .block .title_block</selector>
				<attrs>background-color</attrs>
			</item> 
			<item>
				<type>text</type>
				<label>Heading Color</label>
				<selector>.sidebar .block .title_block a, .sidebar .block .title_block</selector>
				<attrs>color</attrs>
			</item> 
			<item>
				<type>text</type>
				<label>Content Background</label>
				<selector>.sidebar .block .block_content</selector>
				<attrs>background-color</attrs>
			</item>  

			<item>
				<type>text</type>
				<label>Content Color</label>
				<selector>.sidebar .block .block_content</selector>
				<attrs>color</attrs>
			</item> 
			<item>
				<type>text</type>
				<label>Border Color</label>
				<selector>.sidebar .block .block_content, .sidebar .block_content li </selector>
				<attrs>border-color</attrs>
			</item> 
	</items>

	<items>
		<header>Modules highlighted  in Sidebar</header>
		<match>highlighted-modules</match>
			<item>
				<type>text</type>
				<label>Heading Background</label>
				<selector>.sidebar .block.highlighted .title_block</selector>
				<attrs>background-color</attrs>
			</item> 
			<item>
				<type>text</type>
				<label>Heading Color</label>
				<selector>.sidebar .block.highlighted .title_block, .sidebar .block.highlighted .title_block a </selector>
				<attrs>color</attrs>
			</item> 
			<item>
				<type>text</type>
				<label>Content Background</label>
				<selector>.sidebar .block.highlighted .block_content</selector>
				<attrs>background-color</attrs>
			</item>  

			<item>
				<type>text</type>
				<label>Text Color</label>
				<selector>.sidebar .block.highlighted .block_content</selector>
				<attrs>color</attrs>
			</item>
			<item>
				<type>text</type>
				<label>Link Color</label>
				<selector>.sidebar .block.highlighted .block_content a</selector>
				<attrs>color</attrs>
			</item>
			<item>
				<type>text</type>
				<label>Border Color</label>
				<selector>.sidebar .block.highlighted .block_content li, .sidebar .block.highlighted .block_content </selector>
				<attrs>border-color</attrs>
			</item> 
	</items>

</elements>
</themeeditor>
